<div class='container'>
  <%= render partial: '/shared/feed', locals: {feed: @feed} %>

  <% @comments.each do |comment| %>
    <div>
      <p>
        <%= comment.created_by %> <%= comment.displayed_time %>
      </p>
      <p>
        <%= raw comment.text %>
      </p>
    </div>
  <% end %>
</div>