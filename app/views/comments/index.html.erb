<div class='container' style='background-color: #f6f6ef'>
  <%= render partial: '/shared/feed', locals: {feed: @feed} %>
  <br>
  <br>
  <% @comments.each do |comment| %>
    <div>
      <p>
        <%= comment.created_by %> <%= comment.displayed_time %>
      </p>
      <p>
        <%= raw comment.text %>
      </p>
    </div>
  <% end %>
</div>